<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
    <children>
        <TabPane prefHeight="379.0" prefWidth="582.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="schedule" onSelectionChanged="#schedule" text="Schedule">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="matchTable" layoutX="21.0" layoutY="26.0" prefHeight="299.0" prefWidth="287.0">
                                    <columns>
                                        <TableColumn fx:id="matchColumn" prefWidth="163.0" text="Match" />
                                        <TableColumn fx:id="dateColumn" prefWidth="122.0" text="Date " />
                                    </columns>
                                </TableView>
                                <Button fx:id="logoutBtn" layoutX="449.0" layoutY="268.0" mnemonicParsing="false" onAction="#btnBackAction" text="Logout" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="result" text="Results">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView prefHeight="279.0" prefWidth="442.0">
                          <columns>
                            <TableColumn fx:id="team1Column" prefWidth="150.0" text="Team 1" />
                            <TableColumn prefWidth="70.0" text="Score" />
                              <TableColumn prefWidth="70.0" text="Score " />
                              <TableColumn fx:id="team2Column" prefWidth="150.0" text="Team 2" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
                    </content>
                </Tab>
                <Tab text="Standings">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView prefHeight="563.0" prefWidth="862.0">
                           <columns>
                              <TableColumn prefWidth="150.0" text="Team" />
                              <TableColumn prefWidth="150.0" text="Player 1" />
                              <TableColumn prefWidth="150.0" text="Player 2" />
                              <TableColumn minWidth="0.0" prefWidth="68.0" text="Points" />
                              <TableColumn prefWidth="90.0" text="Goals" />
                              <TableColumn prefWidth="54.0" text="MP" />
                              <TableColumn prefWidth="66.0" text="Wins" />
                              <TableColumn prefWidth="58.0" text="Losses" />
                           </columns>
                        </TableView>
                     </children></AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
